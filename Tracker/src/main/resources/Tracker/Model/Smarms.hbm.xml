<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Tracker.Model.Smarms" table="smarms">
        <id name="smarmsId" type="String">
            <column name="SMARMS_ID" />
            <generator class="identity" />
        </id>
        <property name="name" type="string">
            <column name="NAME" length="10" not-null="true" unique="true" />
        </property>
        <property name="tamanoEnBytes" type="java.lang.Integer">
            <column name="SMARMS_TAMANO" length="20" not-null="true" unique="true" />
        </property>
        <set name="peerSmarmses" table="peer_smarmses" inverse="true"
        	lazy="true" fetch="select" cascade="all"  >
            <key>
                <column name="stock" not-null="true" />
            </key>
            <one-to-many class="Tracker.Model.PeerSmarms" />
        </set>
    </class>
</hibernate-mapping>
