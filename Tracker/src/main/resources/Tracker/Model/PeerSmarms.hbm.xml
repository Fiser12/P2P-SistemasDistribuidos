<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Tracker.Model.PeerSmarms" table="peer_smarmses">
		<composite-id name="pk" class="Tracker.Model.PeerSmarmsId" >
			<key-property name="peer" column="peer" />
			<key-property name="smarms" column="smarms" />
		</composite-id>
		<component name="peer">
			<many-to-one name="idPeer" class="Tracker.Model.Peer"
				update="false" insert="false">
				<column name="PEER_ID" not-null="true" />
			</many-to-one>
		</component>
		<component name="smarms">
			<many-to-one name="smarmsId" class="Tracker.Model.Smarms"
				update="false" insert="false">
				<column name="SMARMS_ID" not-null="true" />
			</many-to-one>
		</component>
		<property name="bytesDescargados" type="java.lang.Integer">
			<column name="BYTES_DESCARGADOS" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
